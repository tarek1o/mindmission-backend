{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "packageManager": "npm" }
    },
    {
      "src": "tsconfig.json",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/presentation/(.*)",
      "dest": "dist/presentation/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
